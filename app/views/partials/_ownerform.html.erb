<h1><%= title %></h1>

<%= form_with model: owner do |form| %>

  <%= render "partials/ownererrorform" %>
  <br>
  <%= form.label :owner_name %>
  <%= form.text_field :owner_name %>
  <br>
  <%= form.hidden_field :user_id %>
  <%= form.hidden_field :car_id %>
  <%= form.hidden_field :camper_id %>
  <br>

  <%= form.submit %>
<br>
<% end %>